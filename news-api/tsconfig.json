{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "allowJs": true,
      "checkJs": false,
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  